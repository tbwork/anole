<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.anole.infrastructure.dao.AnoleProjectMapper"> 
  <select id="countProject" parameterType="java.lang.String" resultType="java.lang.Integer">
    select count(*)
    from anole_project
    where `Name` = #{name,jdbcType=VARCHAR}
  </select>
  
  <select id="selectByName" parameterType="java.lang.String" resultMap="BaseResultMap">
    select Id, ProductLine, `Name`, Creator, `Owner`, CreateTime, UpdateTime, Description
    from anole_project
    where `Name` = #{name,jdbcType=VARCHAR}
  </select>
  
  
  
</mapper>